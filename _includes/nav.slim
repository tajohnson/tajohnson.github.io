
.navbar.navbar-static-top
  .container

      button.navbar-toggle type="button" data-toggle="collapse" data-target=".navbar-collapse"
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand href="{{ site.url }}" 
        img src="{{ site.baseurl }}images/logo.png"
     
      .navbar-collapse.collapse
        ul.nav.navbar-nav.nav-links
          | {% for link in site.data.navbar %}
          li: a  href="{% if link.external %}{{ link.url }}{% else %}{{ site.url }}{{ link.url }}{% endif %}" {{ link.title }}
          | {% endfor %}
